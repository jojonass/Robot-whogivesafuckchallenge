<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <loggers>
        <root>
            <level>INFO</level>
        </root>
    </loggers>
    <handlers>
        <handler type="StreamHandler">
            <formatter>simpleFormatter</formatter>
        </handler>
    </handlers>
    <formatters>
        <formatter name="simpleFormatter">
            <format>%(asctime)s - %(name)s - %(levelname)s - %(message)s</format>
        </formatter>
    </formatters>
</configuration>
